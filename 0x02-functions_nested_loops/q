#include "main.h"

/**
 * times_table -  prints the sign of a number.
 *
 * Return: void
 */

void times_table(void)
{
	int i, j, result;
	float k, r;

	k=0;
	r=0;
	for (i = 0; i < 10; i++)
		for(j = 0; j < 10; j++)
		{
			result = i * j ;
			if (result < 10)
			{
				_putchar(result + '0');
				_putchar(',');
				_putchar(' ');
				_putchar(' ');
			}
			else
			{
				k = result / 10;
				r = result % 10;
				_putchar(k + '0');
				_putchar(r + '0');
				_putchar(',');
				_putchar(' ');
				_putchar(' ');
			}
		}
}
